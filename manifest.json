{
  "name": "It's not dead, it's resting",
  "version": "1.1.0",
  "manifest_version": 3,
  "description": "Replaces the ùïè logo in twitter.com with a bird",
  "homepage_url": "https://github.com/slikts/riptwitter",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["https://twitter.com/*"],
      "js": ["lib/twitter.js"],
      "css": ["src/twitter.css"],
      "run_at": "document_end"
    },
    {
      "matches": ["https://tweetdeck.twitter.com/*"],
      "js": ["lib/tweetdeck.js"],
      "css": ["src/tweetdeck.css"],
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["icons/*"],
      "matches": ["<all_urls>"]
    }
  ],
  "content_security_policy": {},
  "host_permissions": ["https://*.twitter.com/*"]
}
